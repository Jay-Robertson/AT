@model Mavo.Assets.Models.ViewModel.JobReportViewModel
@{
    Html.RenderPartial("_ReadyJob", Model.ReadyToPick, new ViewDataDictionary { { "title", "Ready to return today" } });
    Html.RenderPartial("_ReadyJob", Model.ReadyToReturn, new ViewDataDictionary { { "title", "Ready to return today" } });
    Html.RenderPartial("_ReadyJob", Model.BeingPicked, new ViewDataDictionary { { "title", "Jobs being picked right now" } });
    Html.RenderPartial("_ReadyJob", Model.BeingReturned, new ViewDataDictionary { { "title", "Jobs being returned right now" } });
    Html.RenderPartial("_ReadyJob", Model.AlreadyPicked, new ViewDataDictionary { { "title", "Jobs we already picked today" } });
    Html.RenderPartial("_ReadyJob", Model.AlreadyReturned, new ViewDataDictionary { { "title", "Jobs we already returned today" } });
}

